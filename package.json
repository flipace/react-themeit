{
  "name": "react-themeit",
  "version": "1.1.0",
  "description": "themeit makes it easy to create and use different themes for your react components while using CSS Modules or JS style objects.",
  "repository": {
    "type": "git",
    "url": "https://github.com/flipace/react-themeit.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.19.0",
    "aphrodite": "^0.3.1",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "git-url-parse": "^6.0.1",
    "jsdom": "^8.3.1",
    "lodash": "^4.12.0",
    "mocha": "^2.4.5",
    "raw-loader": "^0.5.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0",
    "aphrodite": "^0.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.5.0"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
